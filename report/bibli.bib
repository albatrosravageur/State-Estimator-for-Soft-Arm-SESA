@article{doi:10.1177/003754976500400610,
author = {E.E.L. Mitchell and A.E. Rogers},
title ={Quaternion Parameters in the Simulation of a Spinning Rigid Body},
journal = {SIMULATION},
volume = {4},
number = {6},
pages = {390-396},
year = {1965},
doi = {10.1177/003754976500400610},

URL = { 
