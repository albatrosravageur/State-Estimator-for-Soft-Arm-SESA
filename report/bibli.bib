@article{galloway2019_fiberopt,
author = {Galloway, Kevin C. and Chen, Yue and Templeton, Emily and Rife, Brian and Godage, Isuru S. and Barth, Eric J.},
title = {Fiber Optic Shape Sensing for Soft Robotics},
journal = {Soft Robotics},
volume = {6},
number = {5},
pages = {671-684},
year = {2019},
doi = {10.1089/soro.2018.0131},
    note ={PMID: 31241408},

URL = { 
        https://doi.org/10.1089/soro.2018.0131
    
},
eprint = { 
        https://doi.org/10.1089/soro.2018.0131
    
}
,
    abstract = { Abstract While soft material actuators can undergo large deformations to execute very complex motions, what is critically lacking in soft material robotic systems is the ability to collect high-resolution shape information for sophisticated functions such as environmental mapping, collision detection, and full state feedback control. This work explores the potential of a nearly commercial fiber optic shape sensor (FOSS) and presents the first demonstrations of a monolithic, multicore FOSS integrated into the structure of a fiber-reinforced soft actuator. In this pilot study, we report an open loop sensorized soft actuator capable of submillimeter position feedback that can detect the soft actuator's shape, environmental shapes, collision locations, and material stiffness properties. }
}



